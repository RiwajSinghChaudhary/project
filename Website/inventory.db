CREATE TABLE inventory (
  id INT PRIMARY KEY,
  make VARCHAR(50) NOT NULL,
  model VARCHAR(50) NOT NULL,
  year INT NOT NULL,
  price INT NOT NULL,
  image_url VARCHAR(255) NOT NULL
);

